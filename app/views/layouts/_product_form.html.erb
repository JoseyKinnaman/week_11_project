<%= render "layouts/errors", :object => @product %>

  <%= form_for @product do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :cost %>
  <%= f.text_field :cost %>
  <%= f.label :country_of_origin %>
  <%= f.text_field :country_of_origin %>
  <%= f.label :product_photo %>
  <%= f.file_field :product_photo %>
  <%= f.submit  %>
  <% end %>